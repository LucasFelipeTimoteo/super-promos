{
	"name": "teste-divulgadorinteligente-fullstack",
	"version": "1.0.0",
	"description": "teste de contratação fullstack",
	"main": "src/index.ts",
	"scripts": {
		"scripts": "npm pkg get scripts",
		"start:dev": "nodemon src/index.ts",
		"test": "jest",
		"test:silent": "jest --silent",
		"test:watch": "jest --watchAll",
		"test:e2e": "jest --runInBand --bail --projects ./tests/e2e",
		"test:e2e:silent": "jest --runInBand --bail --silent --projects ./tests/e2e",
		"test:e2e:watch": "jest --runInBand --bail --watchAll --projects ./tests/e2e",
		"lint-staged": "lint-staged",
		"lint:check": "biome lint .",
		"format:fix": "biome format --write .",
		"fix": "biome check --write .",
		"build:dev": "tsc",
		"build:dev:incremental": "tsc --incremental -p tsconfig.json",
		"build": "tsc -p tsconfig.json && node buildUtils/minify.js",
		"revise": "npm-run-all --parallel test:silent test:e2e:silent build:dev fix",
		"revise:pre-commit": "npm-run-all --parallel test:silent test:e2e:silent build:dev:incremental lint-staged",
		"prepare": "husky"
	},
	"keywords": [
		"fullstack",
		"produtos",
		"api"
	],
	"author": "Lucas Felipe Timoteo",
	"license": "ISC",
	"dependencies": {
		"axios": "^1.7.2",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"helmet": "^7.1.0",
		"node-cache": "^5.1.2",
		"pino": "^9.6.0",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@swc/core": "^1.10.1",
		"@swc/jest": "^0.2.37",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.14",
		"@types/node": "^20.14.10",
		"@types/supertest": "^6.0.2",
		"husky": "^9.0.11",
		"jest": "^29.7.0",
		"lint-staged": "^15.2.2",
		"nodemon": "^3.1.0",
		"npm-run-all": "^4.1.5",
		"pino-pretty": "^11.3.0",
		"sucrase": "^3.35.0",
		"supertest": "^7.0.0",
		"typescript": "^5.5.3",
		"uglify-js": "^3.17.4"
	}
}
